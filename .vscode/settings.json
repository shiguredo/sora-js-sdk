{
  "[typescript]": {
    "editor.rulers": [
      100
    ],
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[markdown]": {
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "editor.detectIndentation": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "deno.enable": false,
  "deno.lint": true,
  "deno.config": "./deno.jsonc",
}